{
  "name": "anm",
  "description": "Page animations by mouse and touch moves",
  "version": "0.0.0",
  "repository": "andrepolischuk/anm",
  "dependencies": {
    "anm": "^2.0.1",
    "bootstrap-gh-pages": "^0.2.1"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.3",
    "postcss": "^4.1.11",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2"
  },
  "scripts": {
    "prebuild": "rm -rf build && mkdir build",
    "build": "npm run build-js && npm run build-css",
    "build-js": "browserify --transform babelify index.js > build/index.js",
    "build-css": "postcss --use postcss-import index.css > build/index.css"
  },
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT"
}
